{
    "name": "Android Assets",
    "description": "Sketch plugin for generating Android assets.",
    "author": "Marius Mathiesen",
    "homepage": "https://github.com/zmalltalker/sketch-android-assets/",
    "version": 1.5,
    "identifier": "no.shortcut.android-assets-plugin",
    "compatibleVersion": "3.4.2",
    "bundleVersion": 1,
    "commands": [
        {
            "name": "Generate Android Assets",
            "identifier": "android-assets",
            "shortcut": "cmd shift a",
            "script": "Generate Android Assets.cocoascript",
            "handler": "generateAndroidAssets"
        },
        {
            "name": "Windows Assets",
            "shortcut": "cmd shift w",
            "identifier": "windows-assets",
            "script": "Generate Windows Phone Assets.cocoascript"
        }
    ],
    "menu": {
        "items": [
            "android-assets",
            "windows-assets"
        ]
    }
}
